	<!-- Objective -->
	<div class="container">
		<h1 class="title_header">Objective</h1>
		<div class="content">
			<span class="CS">To obtain a position in the computer science field in order to apply and enhance knowledge gained through my passion for computer science.</span></br>
			<span class="ME">To obtain a position in the field of mechanical engineering.</span>
		</div>
	</div>
	
	
	
	<!-- Technical Skills -->
	<div class="container">
		<h1 class="title_header">Technical Skills</h1>
		<!--<div class="header">expand</div>-->
		<div class="content">
			<table class="borders">
				<tr class="CS-block">
					<td>applications</td><td>vim, Microsoft Visual Studio, Eclipse + ADT plugin, Microsoft Office</td>
				</tr>
				<tr class="ME-block">
					<td>applications</td><td>SolidWorks, AutoCAD Inventor, Ansys Fluent, StarCCM+, LabVIEW, Microsoft Office</td>
				</tr>
				<tr>
					<td>systems</td><td>Microsoft Windows, Red Hat, Ubuntu, Debian</td>
				</tr>
				<tr class="">
					<td>programming languages</td><td>C/C++, Python, MatLab, x86 assembly, Java, VBA, HTML, Javascript, PHP, SQL, TCL, make, CMake, bash</td>
				</tr>
			</table>
		</div>
	</div>
	
	<!-- Personal Projects -->
	<div class="container CS-block">
		<h1 class="title_header">Personal Projects</h1>
		<div class="content">
			<a class="web" href="http://www.github.com/chuck1">github</a>
			<span class="print">http://www.github.com/chuck1</span>
		</div>
	</div>
	
	<!-- Education -->
	<div class="container">
		<h1 class="title_header">Education</h1>
		<div class="content">
			<table class="cols">
				<tr>
					<td><b>Bachelor of Science in Mechanical Engineering</b></td><td></td>
				</tr>
				<tr>
					<td>Magna Cum Laude</td><td></td>
				</tr>
				<tr>
					<td>Oregon State University</td><td>Corvallis, Oregon</td>
				</tr>
				<tr>
					<td>Start Date: September 24, 2007</td><td></td>
				</tr>
				<tr>
					<td>Graduation Date: March 23, 2012</td><td>GPA: 3.78</td>
				</tr>
				<tr>
					<td></br></td>
				</tr>
				<tr>
					<td><b>Master of Science in Mechanical Engineering</b></td><td></td>
				</tr>
				<tr>
					<td>Oregon State University</td><td>Corvallis, Oregon</td>
				</tr>
				<tr>
					<td>Start Date: April 2, 2012</td>
				</tr>
				<tr>
					<td>Graduation Date: March 20, 2015</td><td>GPA: 3.91</td>
				</tr>
			</table>
		</div>
	</div>
	

	<!-- Employment -->
	<!-- page-break-inside property not working in Chrome, use manual page break instead -->
	<div class="container" style="page-break-before: always">
		<h1 class="title_header">Employment</h1>
		<div class="content">
			<table class="cols">
				{% for e in emp %}
				<tr>
					<td><b>{{ e.title }}</b></td>
					<td><b>{{ e.duration }}</b></td>
				</tr>
				<tr>
					<td><b>{{ e.employer }}</b> - {{ e.location }}</td>
					<td></td>
				</tr>
				<tr>
					<td colspan="2" class="CS">
						<ul>
							{% for l in e.desc_cs %}<li>{{ l }}</li>
							{% endfor %}
						<ul>
					</td>
					<td colspan="2" class="ME">
						<ul>
							{% for l in e.desc_me %}<li>{{ l }}</li>
							{% endfor %}
						</ul>
					</td>
				</tr>
				<tr><td></br></td></tr>
				{% endfor %}
			</table>
		</div>
	</div>
	
</html>


